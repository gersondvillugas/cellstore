        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity"></activity>
        <activity android:name=".activities.auth.RegisterActivity"></activity>
        <activity android:name=".activities.auth.LoginActivity" />
        <activity android:name=".activities.Main2Activity" />
        <activity android:name=".activities.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ResultadoBusqueda" />
        <activity android:name=".activities.ResultadoTablet">

